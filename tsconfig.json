{
    "compilerOptions": {
        "types": [
            "utools-api-types",
            "@types/node"
        ],
        "composite": true,
        "module": "CommonJS",
        "target": "ES5",
        "esModuleInterop": false,
        "moduleResolution": "Node",
        "outDir": "dist",
        "sourceMap": false
    },
    "include": [
        "src"
    ],
    "exclude": [
        "dist",
        "tests",
        "dist-tests"
    ]
}
